         function validateDaysNo(input) {
             let maximumNoDays = 365;

             input.value = input.value.replace(/[^0-9]/g, '');
             let value = parseInt(input.value);

             if (isNaN(value)) {
                 input.value = '';
             } else if (value > maximumNoDays) {
